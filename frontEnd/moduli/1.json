            },
            {
                "type": "text",
                "name": "codice_fiscale_genitore",
                "domanda": "Codice Fiscale del genitore/tutore",
                "reg": "[a-zA-Z]{3}[a-zA-Z]{3}[0-9]{2}[a-zA-Z]{1}[0-9]{2}[a-zA-Z]{1}[0-9]{3}[a-zA-Z]{1}"
            },
            {
                "type": "file",
                "name": "foto_codice_fiscale_genitore",
                "domanda": "Foto del codice fiscale del genitore/tutore",
                "descrizione": "Allegare un file in formato .pdf, .png o .jpg"
            },
            {
                "type": "button",
                "section": "4"
            }
        ],
